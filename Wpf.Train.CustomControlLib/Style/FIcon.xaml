<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Wpf.Train.CustomControlLib">
        <!--FIcon-->
    <Style x:Key="FIcon" TargetType="{x:Type TextBlock}">
        <Setter Property="FontFamily" Value="/Wpf.Train.CustomControlLib;component/ImgResources/FontImage/#SF2015"></Setter>
        <Setter Property="Foreground" Value="{DynamicResource ButtonForeground}"/>
        <Setter Property="TextAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="FontSize" Value="20"/>
    </Style>
</ResourceDictionary>